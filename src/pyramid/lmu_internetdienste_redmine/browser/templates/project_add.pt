</div>
    <script src="${request.static_url('pyramid.redmine:static/js/jquery-1.11.1.min.js')}"></script>
    <script src="${request.static_url('pyramid.redmine:static/js/foundation.min.js')}"></script>
    <script type="text/javascript">
        deform.load()
